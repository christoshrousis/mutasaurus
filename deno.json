{
  "name": "@mutasaurus/mutasaurus",
  "version": "0.0.1",
  "exports": "./src/mod.ts",
  "tasks": {
    "test": "deno test --allow-read --allow-write --allow-run",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "ok": "deno run --allow-read --allow-write --allow-run --allow-env --allow-ffi ./test/test.ts"
  },
  "fmt": {
    "include": [
      "src/",
      "tests/"
    ]
  },
  "lint": {
    "include": [
      "src/",
      "tests/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert",
    "@std/fs": "jsr:@std/fs@1.0.15",
    "oxc-parser": "npm:oxc-parser",
    "oxc-walker": "npm:oxc-walker@0.2.3",
    "oxc-project-types": "npm:@oxc-project/types@0.61.1"
  }
}
