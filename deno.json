{
  "name": "@mutasaurus/mutasaurus",
  "version": "0.1.4",
  "exports": "./mod.ts",
  "tasks": {
    "clean": "rm -rf ./.mutasaurus %% rm -rf ./coverage",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "fixtures": "deno test --allow-all ./test/fixtures/",
    "ok": "deno test --allow-all ./test/ok.test.ts && deno publish --dry-run"
  },
  "fmt": {
    "include": [
      "src/",
      "tests/"
    ]
  },
  "lint": {
    "include": [
      "src/",
      "tests/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.15",
    "@std/fs": "jsr:@std/fs@^1.0.19",
    "oxc-walker": "npm:oxc-walker@0.5.2",
    "oxc-project-types": "npm:@oxc-project/types@0.95.0"
  }
}
